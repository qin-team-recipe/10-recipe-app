{
  "name": "recipe-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "lint:precommit": "eslint 'src/**/*.{ts,tsx}'",
    "fmt:precommit": "prettier --write './src/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "prepare": "husky install",
    "migrate": "prisma format && prisma migrate dev --name",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "yarn run lint:precommit",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": "yarn run fmt:precommit"
  },
  "dependencies": {
    "@prisma/client": "5",
    "@supabase/auth-helpers-nextjs": "^0.7.2",
    "@supabase/supabase-js": "^2.24.0",
    "@tabler/icons-react": "^2.23.0",
    "next": "13.4.4",
    "prisma": "5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "supabase": "^1.64.8",
    "tabler-icons-react": "^1.56.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.0.0-beta.0",
    "@storybook/addon-essentials": "^7.2.3",
    "@storybook/addon-interactions": "^7.2.3",
    "@storybook/addon-links": "^7.2.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/addon-styling": "^1.3.6",
    "@storybook/blocks": "^7.2.3",
    "@storybook/nextjs": "^7.2.3",
    "@storybook/react": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/parser": "^5.59.6",
    "autoprefixer": "10.4.14",
    "classcat": "^5.0.4",
    "encoding": "^0.1.13",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "storybook": "^7.2.3",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  }
}
